    "@rails/webpacker": "4.2.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-polyfill": "^6.26.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-semantic-toasts": "^0.6.5",
    "react_ujs": "^2.6.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.2",
    "turbolinks": "^5.2.0",
    "whatwg-fetch": "^3.0.0"
  },
  "version": "0.1.0",
  "devDependencies": {
    "@babel/preset-env": "^7.9.0",
    "babel-jest": "^25.2.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "jest": "^25.2.3",
    "react-test-renderer": "^16.13.1",
    "redux-devtools-extension": "^2.13.8",
    "webpack-dev-server": "^3.10.3"
  },
  "scripts": {
    "build": "yarn install",
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "jest": {
    "setupFiles": [
      "./app/javascript/setupTests.js"
    ],
    "roots": [
      "<rootDir>/app/javascript/components/__tests__"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/app/javascript/components/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/app/javascript/components/__mocks__/styleMock.js"
    }
  }
